11. //WAPto reverse string using pointers
//Friend -> dneirF
#include <stdio.h>
#include <string.h>

int main()
{
    char arr[]="friend";
    char *start=arr;
    char *end=arr + strlen(arr) - 1;
    char temp;
    printf("Before = %s\n", arr);

    while (start<end)
    {
        temp=*start;
        *start=*end;
        *end=temp;
        start++;
        end--;
    }

    printf("After = %s\n", arr);

    return 0;
}
