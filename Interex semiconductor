13.
#include<stdio.h>
#include<stdlib.h>
int val[]={20,5,30,40,50};

int *func()
{
return val;
}

void main()
{
*func()=val;
printf("%d\n", ++((func()[3])));
}
14.#include<stdio.h>
int main()
{

int *p1[5];    
int *(p2[5]);  
int (*p3)[5];  
printf(" sizeof *p1[3]=%d\n",   sizeof(p1));
printf(" sizeof *(p2[3])=%d\n", sizeof(p2));
printf(" sizeof *p3)[3] =%d\n", sizeof(p3));
}
